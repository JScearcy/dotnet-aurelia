{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (web)",
            "type": "node",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceRoot}/bin/Debug/netcoreapp1.0/dotnet-aurelia.dll",
            "args": [],
            "cwd": "${workspaceRoot}",
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            }
        },
        {
            "name": ".NET Core Attach",
            "type": "node",
            "request": "attach",
            "processId": "${command.pickProcess}"
        }
    ]
}